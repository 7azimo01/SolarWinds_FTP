<!-- **************** Advanced Limits **************** -->
$LIMIT_ADV_CONVERTURLCHARS$=Convert URL characters in commands to ASCII
$LIMIT_ADV_CHECKDIRSIZE$=Automatically check directory sizes during upload
$LIMIT_ADV_INLINEOOB$=Inline out-of-band data
$LIMIT_ADV_KEEPALIVE$=Send keep-alive packets to detect broken connections
$LIMIT_ADV_NAGLE$=Disable usage of Nagle algorithm
$LIMIT_ADV_ADAPTIVETO$=Use adaptive time-out on fast file uploads
$LIMIT_ADV_SFTPMAXVERSION$=Maximum supported SFTP version
$LIMIT_ADV_ALLOWRENAMEOVERWRITE$=Allow rename overwrite
$LIMIT_ADV_WARNOLDBROWSER$=Warn end users when using old web browsers
$LIMIT_ADV_DEFAULT_LANG$=Default language for Web Client
$LIMIT_ADV_SERV_DOM_DIR_ACCESS$=Apply server and domain directory access rules before user and group
$LIMIT_ADV_PRE_DISABLE_DAYS$=Days before automatically disabling account to trigger the pre-disable event
$LIMIT_ADV_PRE_DELETE_DAYS$=Days before automatically deleting account to trigger the pre-delete event
$LIMIT_ADV_FILE_OWNER$=Owner ID (user name) for created files and directories
$LIMIT_ADV_FILE_GROUP$=Group ID (group name) for created files and directories
$LIMIT_ADV_RESET_USER_STATS_RESTART$=Reset user stats after restart
$LIMIT_ADV_RESET_GROUP_STATS_RESTART$=Reset group stats after restart
$ALLOW_NON_RFC_SSH_PROTOCOL_VERSION_EXCHANGE_ID$=Allow non-RFC compliant SSH protocol version exchange

<!-- **************** Connection Limits **************** -->
$LIMIT_CONN_MAXSERVERSESS$=Maximum number of sessions on server
$LIMIT_CONN_MAXSERVERSESSIP$=Maximum sessions per IP address on server
$LIMIT_CONN_MAXDOMAINSESS$=Maximum number of sessions on domain
$LIMIT_CONN_MAXDOMAINSESSIP$=Maximum sessions per IP address on domain
$LIMIT_CONN_MAXGROUPSESS$=Maximum number of sessions for group
$LIMIT_CONN_MAXGROUPSESSIP$=Maximum sessions per IP address for group
$LIMIT_CONN_MAXSESSUSER$=Maximum number of sessions per user account
$LIMIT_CONN_MAXSESSIPUSER$=Maximum sessions per IP address for user account
$LIMIT_CONN_REQUIRESECURE$=Require secure connection before login
$LIMIT_CONN_IDLETO$=Automatic idle connection timeout
$LIMIT_CONN_SESSTO$=Automatic session timeout
$LIMIT_CONN_BLOCKANTITO$=Block anti-timeout schemes
$LIMIT_CONN_AUTOCREATE$=Automatically create home directories
$LIMIT_CONN_TIMEOUTBLOCKIP$=Block IP address of timed out session
$LIMIT_CONN_FTP$=Allow FTP and FTPS connections
$LIMIT_CONN_SSH$=Allow SFTP connections
$LIMIT_CONN_HTTP$=Allow HTTP and HTTPS connections
$LIMIT_CONN_REQ_REV_DNS$=Require reverse DNS name
$LIMIT_CONN_ALLOWXFORWARD$=Allow X-Forwarded-For to change HTTP connection IP addresses

<!-- **************** Directory Listing Limits **************** -->
$LIMIT_DL_HIDEHIDDEN$=Hide files marked as hidden from listings
$LIMIT_DL_LOWERCASE$=Use lowercase for file names and directories
$LIMIT_DL_SCLINKS$=Interpret Windows shortcuts as links
$LIMIT_DL_SCLINKS_NON_WIN_OS$=Interpret symbolic links as links
$LIMIT_DL_SCASTARGET$=Treat Windows shortcuts as target in listings
$LIMIT_DL_SCASTARGET_NON_WIN_OS$=Treat symbolic links as target in listings
$LIMIT_DL_LOCK$=Lock users in home directory
$LIMIT_DL_ROOT$=Allow root (\"/\") to list drives for unlocked users
$LIMIT_DL_COMP_STATE$=Hide the compressed state of files and directories
$LIMIT_DL_ENCRYPT_STATE$=Hide the encrypted state of files and directories
$LIMIT_DL_NATURALSORT$=Use natural language sorting for sorted listings
$LIMIT_DL_DIR_MSG$=Message file path
$LIMIT_DL_DIR_LIST_MASK$=Directory listing mask

<!-- **************** Password Limits **************** -->
$LIMIT_PW_CHECKANON$=Check anonymous passwords
$LIMIT_PW_REQCOMPLEX$=Require complex passwords
$LIMIT_PW_MINLENGTH$=Minimum password length
$LIMIT_PW_EXPIRES$=Automatically expire passwords
$LIMIT_PW_CHANGE$=Allow users to change password
$LIMIT_PW_ENCRYPT$=Store passwords in encrypted form
$LIMIT_PW_HIDEINLOG$=Mask received passwords in logs
$LIMIT_PW_SSHAUTHTYPE$=SSH authentication type
$LIMIT_PW_TYPE$=FTP password type
$LIMIT_PW_ENCRYPT_MODE$=Password encryption mode
$LIMIT_PW_DISABLE_GENERATORS$=Disable password generators
$LIMIT_PW_ENABLE_RECOVERY$=Allow users to recover passwords
$LIMIT_PW_STALE_DAYS$=Days before considering password to be stale


<!-- **************** Transfer Limits **************** -->
$LIMIT_TRAN_SERVERUP$=Maximum upload speed for server
$LIMIT_TRAN_SERVERDN$=Maximum download speed for server
$LIMIT_TRAN_DOMAINUP$=Maximum upload speed for domain
$LIMIT_TRAN_DOMAINDN$=Maximum download speed for domain
$LIMIT_TRAN_GROUPUP$=Maximum upload speed for all group members
$LIMIT_TRAN_GROUPDN$=Maximum download speed for all group members
$LIMIT_TRAN_USERUP$=Maximum upload speed for user accounts
$LIMIT_TRAN_USERDN$=Maximum download speed for user accounts
$LIMIT_TRAN_SESSUP$=Maximum upload speed per session
$LIMIT_TRAN_SESSDN$=Maximum download speed per session
$LIMIT_TRAN_DELETEPARTIAL$=Delete partially uploaded files
$LIMIT_TRAN_MAX_FILE_SIZE$=Maximum upload file size
$LIMIT_TRAN_INTERPRETLFASEOL$=Interpret line feed byte as a new line when in ASCII mode
$LIMIT_TRAN_ALLOWMEDIAPLAY$=Allow HTTP media playback
$LIMIT_TRAN_UPLOAD_FILE_MODE$=File upload access
$LIMIT_TRAN_DOWNLOAD_FILE_MODE$=File download access
$LIMIT_TRAN_MAX_SIMUL_THUMBS$=Maximum simultaneous thumbnails


<!-- **************** HTTP Limits **************** -->
$LIMIT_HTTP_ALLOW_REMEMBER_ME$=Allow browsers to remember login information
$LIMIT_HTTP_ALLOW_SHOW_THEMES$=Allow users to change themes
$LIMIT_HTTP_ALLOW_SHOW_LANG$=Allow users to change languages
$LIMIT_HTTP_ALLOW_WEB_CLIENT$=Allow users to use Web Client
$LIMIT_HTTP_ALLOW_WEB_CLIENT_PRO$=Allow users to use Web Client Pro
$LIMIT_HTTP_ALLOW_FVJV$=Allow users to use FTP Voyager JV
$LIMIT_HTTP_MAINTAIN_UL_TIMES$=Maintain file dates and times after uploading (FTP Voyager JV and Web Client Pro only)
$LIMIT_HTTP_ALLOW_IP_CHANGE$=Allow HTTP sessions to change IP address
$LIMIT_HTTP_DISABLE_BRANDING$=Disable web-based client branding
$LIMIT_HTTP_HIDE_VERSION_IN_HEADER$=Hide Serv-U version information in HTTP response headers
$LIMIT_HTTP_ALLOW_AUTOCOMPLETE_IN_LOGIN_FIELDS$=Allow autocomplete for HTTP login fields
$LIMIT_HTTP_USE_HSTS$=Use HTTP Strict Transport Security
$LIMIT_HTTP_DISABLE_COMPRESSION$=Disable HTTP compression
$LIMIT_UPLOADED_FILE_FLUSH_PERIOD$=Periodic buffer flush interval during SFTP file upload
$LIMIT_ALLOW_PERMITTED_CROSS_DOMAIN_POLICIES$=Allow Permitted Cross Domain Policies


<!-- **************** Email Limits **************** -->
$LIMIT_EMAIL_REQUIRE_SET$=Require end-user to set email address at next HTTP login
$LIMIT_EMAIL_ALLOW_SET$=Allow end-user to set account email address


<!-- **************** File Sharing Limits **************** -->
$LIMIT_ADHOC_REQUIRE_PWORD_FOR_GUESTS$=Require a password for guest access
$LIMIT_ADHOC_INSERT_PWORD_IN_EMAIL$=Insert passwords within invitation emails
$LIMIT_ADHOC_MAX_GUEST_UPLOAD_FILE_SIZE$=Maximum file size guests can upload (per file)
$LIMIT_ADHOC_ALLOW_USER_DEF_GUEST_LINK_EXPIRATION$=Allow user-defined guest link expiration
$LIMIT_ADHOC_MAX_GUEST_LINK_EXPIRATION$=Maximum duration before guest link expires
$LIMIT_ADHOC_NOTIFY_AFTER_GUEST_DOWNLOAD$=Notify user after a file is downloaded
$LIMIT_ADHOC_NOTIFY_AFTER_GUEST_UPLOAD$=Notify user after a file is uploaded
$LIMIT_ADHOC_SEND_LINK_TO_RECIPIENTS$=Send the guest access link to the recipients
$LIMIT_ADHOC_SEND_LINK_TO_SENDER$=Send the guest access link to the sender
$LIMIT_ADHOC_ALLOW_FILE_SHARING$=Allow file sharing
$LIMIT_ADHOC_ALLOW_CUSTOM_CONTACT_INFO$=Allow user-defined contact information
$LIMIT_ADHOC_MAX_FILES_IN_SENT_SHARE$=Maximum number of files for \"Sent\" shares
$LIMIT_ADHOC_MAX_FILES_IN_REQUESTED_SHARE$=Maximum number of files for \"Requested\" shares
$LIMIT_ADHOC_ALLOWED_OUTGOING_SOURCE$=Allowed \"Sent\" file share sources
$ADHOC_ADHOC_ALLOWED_OUTGOING_SOURCE_0$=Any source
$ADHOC_ADHOC_ALLOWED_OUTGOING_SOURCE_1$=Local files on \"My Computer\"
$ADHOC_ADHOC_ALLOWED_OUTGOING_SOURCE_2$=Remote files on \"Serv-U\"


