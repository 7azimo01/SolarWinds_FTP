<?xml version="1.0" encoding="UTF-8" ?>
<jnlp spec="1.0+" codebase="%HEADER_URL%%COMMON_FILE%Java/">
<information>
<title>$WCP_TITLE$</title>
<vendor>$WCP_VENDER$</vendor>
<icon kind="splash" href="Images/WCPro_64x64.png" width="64" height="64" />
<icon href="Images/WCPro_48x48.png" width="48" height="48" />
<icon href="Images/WCPro_32x32.png" width="32" height="32" />
<icon href="Images/WCPro_24x24.png" width="24" height="24" />
</information>
<security>
<all-permissions />
</security>
<resources>
<j2se version="1.7+" href="http://java.sun.com/products/autodl/j2se" />
<jar href="WebClientPro.jar" download="eager" main="true" />
<jar href="Libs/RhinoJavaCore.jar" download="eager" />
</resources>
<applet-desc name="$WCP_TITLE$" main-class="com.rhinosoft.WebClientPro.CWebClientProApplet" width="500" height="500">
<param name="-hh" value="%HEADER_HOST%" />					
