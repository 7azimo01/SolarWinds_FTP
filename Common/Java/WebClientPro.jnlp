<param name="-hp" value="%HEADER_PORT%" />					
<param name="-esi" value="%ENC_SESSION_ID%" />				
<param name="-li" value="%LOGIN_ID%" />						
<param name="-hl" value="%HEADER_LANGUAGE%" />				
<param name="-is" value="%IS_SECURE%" />					
<param name="-ila" value="%IS_LOCAL_ADMIN%" />				
<param name="-hbu" value="%HELP_BASE_URL%" />				
</applet-desc>
</jnlp>
